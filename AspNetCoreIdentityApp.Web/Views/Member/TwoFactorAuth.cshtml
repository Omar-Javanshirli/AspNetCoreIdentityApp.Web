@{
    ViewData["Title"] = "TwoFactorAuth";
}

@model TwoFactorAuthViewModel

<h2>Iki adimli kimlik Dogrulama</h2>

<div class="row">
    <div class="col-md-12">

        @if (TempData["SuccessMessage"] != null)
        {
            <div class="alert alert-info" role="alert">
                @TempData["SuccessMessage"]
            </div>
        }

        <form asp-action="TwoFactorAuth" method="post">

            <div class="form-group">
                <label asp-for="TwoFactorType" class="control-label"></label>
                <select class="from-control" asp-for="TwoFactorType" asp-items="Html.GetEnumSelectList<TwoFactor>()"></select>
            </div>

            <div class="form-group">
                <input type="submit" value="Guncelle" class="btn btn-success" />
            </div>

        </form>
    </div>

 
</div>
